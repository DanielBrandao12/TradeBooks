<div class="container-endereco">
    <button class="btnUserProfile" id="abrirFormEndereco">Adicionar novo endereço</button>
    <div id="formEnde">

        <form method="post" action="/userProfile/addAddress" id="formSalva">
            <label for="cep">CEP</label>
            <input id="cep" type="text" name="cep" placeholder="Digite o CEP aqui" />
        
            <label for="street">Rua</label>
            <input id="street" type="text" name="rua" placeholder="Digite a rua aqui" />

            <label for="number">Número</label>
            <input id="number" type="text" name="numero" placeholder="Digite a rua aqui" />

            <label for="complemento">Complemento</label>
            <input id="complemento" type="text" name="complemento" placeholder="Digite a rua aqui" />
        
            <label for="neighborhood">Bairro</label>
            <input id="neighborhood" type="text" name="bairro" placeholder="Digite o bairro aqui" />
        
            <label for="city">Cidade</label>
            <input id="city" type="text" name="cidade" placeholder="Digite a cidade aqui" />
        
            <label for="state">Estado</label>
            <input id="state" type="text" name="estado" placeholder="Digite o estado aqui" />
        
            <button class="btnUserProfile">Salvar</button>
        </form>

        <form method="post" action="" id="formEdita">
         
            <label for="cep">CEP</label>
            <input id="cep2" type="text" name="cep" placeholder="Digite o CEP aqui" />
        
            <label for="street">Rua</label>
            <input id="street2" type="text" name="rua" placeholder="Digite a rua aqui" />

            <label for="number">Número</label>
            <input id="number2" type="text" name="numero" placeholder="Digite a rua aqui" />

            <label for="complemento">Complemento</label>
            <input id="complemento2" type="text" name="complemento" placeholder="Digite a rua aqui" />
        
            <label for="neighborhood">Bairro</label>
            <input id="neighborhood2" type="text" name="bairro" placeholder="Digite o bairro aqui" />
        
            <label for="city">Cidade</label>
            <input id="city2" type="text" name="cidade" placeholder="Digite a cidade aqui" />
        
            <label for="state">Estado</label>
            <input id="state2" type="text" name="estado" placeholder="Digite o estado aqui" />
        
            <button class="btnUserProfile">Salvar Edição</button>
        </form>

        <button class="btnUserProfile" id="cancelarEndereco">Cancelar</button>
    </div>
<div class="div-meus-enderecos">
    <h1>Lista de endereços</h1>
    
    <div class="container-enderecos">
        <% for(let enderecos of myAddress){ %>
        <div class="enderecos">
            <!--Aqui lista de endereços onde posso editar ou excluir um endereço-->
            <div class="spanEndereco" id="<%=enderecos.dataValues.id%>">
                <div>
    
                    <span id="spanE<%=enderecos.dataValues.id%>"><%=enderecos.dataValues.RUA%>,</span>
                    <span id="spanE<%=enderecos.dataValues.id%>"><%=enderecos.dataValues.NUMERO%></span>
    
                </div>
                <div>
    
                    <span id="spanE<%=enderecos.dataValues.id%>"><%=enderecos.dataValues.BAIRRO%>,</span>
                    <span id="spanE<%=enderecos.dataValues.id%>"><%=enderecos.dataValues.CIDADE%> - </span>
                    <span id="spanE<%=enderecos.dataValues.id%>"><%=enderecos.dataValues.ESTADO%></span>
                </div>
                <span id="spanE<%=enderecos.dataValues.id%>">CEP:<%=enderecos.dataValues.CEP%></span>
            </div>
            <div class="button">
                <button id="editar" class="<%=enderecos.dataValues.id%>">Editar</button>
                <form method="post" action="/userProfile/deletarAddress/<%=enderecos.dataValues.id%>?_method=DELETE">

                    <button >Excluir</button>
                </form>
            </div>
        </div>
    
        <% } %>
    </div>
 
    
</div>
</div>